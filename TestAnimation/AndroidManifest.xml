<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cytmxk.testanimation"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="20"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity android:name="com.cytmxk.testanimation.TestAnimationActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.cytmxk.testanimation.view.ChangedActivity"></activity>
        <activity android:name="com.cytmxk.testanimation.view.LayoutActivity"></activity>
    </application>

</manifest>
